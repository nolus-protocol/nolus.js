{
    "name": "@nolus/nolusjs",
    "author": "Nolabs",
    "license": "Apache-2.0",
    "version": "3.7.3",
    "description": "JS library for NodeJS and Web browsers to interact with the Nolus Protocol",
    "engines": {
        "node": ">=14.0.0"
    },
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "files": [
        "build/",
        "*.md"
    ],
    "scripts": {
        "build": "yarn build:module",
        "build:module": "tsc",
        "lint": "eslint '**/*.{ts,js}'",
        "bump": "npm version",
        "generate-docs": "npx typedoc src/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/N-Stealth/nolusjs.git"
    },
    "keywords": [
        "nolus-protocol",
        "typescript",
        "blockchain",
        "cosmos",
        "tendermint"
    ],
    "bugs": {
        "url": "https://github.com/N-Stealth/nolusjs/issues"
    },
    "homepage": "https://github.com/N-Stealth/nolusjs#readme",
    "dependencies": {
        "@bufbuild/protobuf": "^2.10.0",
        "@cosmjs/cosmwasm-stargate": "0.37.0",
        "@cosmjs/crypto": "0.37.0",
        "@cosmjs/encoding": "0.37.0",
        "@cosmjs/ledger-amino": "0.37.0",
        "@cosmjs/proto-signing": "0.37.0",
        "@cosmjs/stargate": "0.37.0",
        "@cosmjs/tendermint-rpc": "0.37.0",
        "@keplr-wallet/crypto": "0.12.290",
        "@keplr-wallet/unit": "0.12.290",
        "@ledgerhq/hw-app-cosmos": "^6.32.9",
        "@ledgerhq/hw-transport-webhid": "^6.30.9",
        "@ledgerhq/hw-transport-webusb": "^6.29.13",
        "@types/crypto-js": "^4.2.2",
        "@types/elliptic": "^6.4.18",
        "@types/uuid": "^10.0.0",
        "crypto-browserify": "^3.12.0",
        "crypto-js": "^4.2.0",
        "typedoc": "^0.28.10"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^8.46.2",
        "@typescript-eslint/parser": "^8.46.2",
        "eslint": "^9.38.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.5.4",
        "prettier": "^3.6.2",
        "typescript": "~5.9.2"
    }
}
